import rclpy
from sensor_msgs.msg import NavSatFix
from sensor_msgs.msg import LaserScan


class MyNode(Node):
    def __init__(self):
        super().__init__('my_node')
        self.vectornav_subscriber = self.create_subscription(NavSatFix, 'vectornav/gnss', self.vectornav_callback, 10)
        self.urg_subscriber = self.create_subscription(LaserScan, '/scan', self.urg_callback, 10)

    def vectornav_callback(self, msg):
        # Process VectorNav data
        pass

    def urg_callback(self, msg):
        # Process URG data
        pass

def main(args=None):
    rclpy.init(args=args)
    node = MyNode()
    rclpy.spin(node)
    rclpy.shutdown()

if __name__ == '__main__':
    main()